#!/sbin/sh

abi="$(grep -m 1 'ro.product.cpu.abi=' /system/build.prop /default.prop 2>/dev/null | cut -d'=' -f2)"

if [ -z "${abi}" ]; then
    abi=armeabi-v7a
fi

sed "1,/BEGIN_${abi}/d;/END_${abi}/,\$d" "${0}" | base64 -d > /tmp/miniunzip
chmod 755 /tmp/miniunzip

/tmp/miniunzip "${@}"

exit ${?}
