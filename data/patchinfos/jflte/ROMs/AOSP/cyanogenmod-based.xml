<patchinfo>
    <name>CyanogenMod-based ROM</name>
    <regexes>
    	<regex>^cm-[0-9\.]+-[0-9]+-.*.zip$</regex>
    	<exclude-regex>noobdev</exclude-regex>
	</regexes>

    <ramdisk>jflte/default</ramdisk>
    <autopatchers>
        <autopatcher>StandardPatcher</autopatcher>
        <autopatcher>DalvikCachePatcher</autopatcher>
    </autopatchers>
</patchinfo>
