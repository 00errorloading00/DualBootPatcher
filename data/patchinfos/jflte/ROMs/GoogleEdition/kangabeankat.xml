<patchinfo>
    <name>Kangabean/Kangakat</name>
    <regex>^K[BK]-.*\.zip$</regex>

    <ramdisk>jflte/default</ramdisk>

    <matches regex="KK.*">
        <autopatchers>
            <autopatcher>StandardPatcher</autopatcher>
        </autopatchers>
    </matches>

    <matches regex="KB.*">
        <autopatchers>
            <autopatcher>StandardPatcher</autopatcher>
        </autopatchers>
    </matches>
</patchinfo>
