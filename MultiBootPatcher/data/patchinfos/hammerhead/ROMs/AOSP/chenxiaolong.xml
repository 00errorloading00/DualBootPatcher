<patchinfo>
    <name>chenxiaolong's noobdev CyanogenMod</name>
    <regex>^cm-.*noobdev.*\.zip$</regex>

    <ramdisk>hammerhead/AOSP/cxl</ramdisk>
    <autopatchers>
    	<autopatcher>NoobdevMultiBoot</autopatcher>
        <autopatcher>StandardPatcher</autopatcher>
        <autopatcher>NoobdevSystemProp</autopatcher>
    </autopatchers>
    
    <!-- The ROM has built-in dual boot support -->
    <partconfigs>
    	<include>all</include>
    	<exclude>dual</exclude>
    </partconfigs>
</patchinfo>
