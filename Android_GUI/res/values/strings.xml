<resources>
    <!-- Name of the app built in release mode -->
    <string name="app_name_release">Dual Boot Patcher</string>
    <!-- Name of the app built in debug mode -->
    <string name="app_name_debug">Dual Boot Patcher (Debug)</string>
    <!-- Name of the app built in snapshot mode -->
    <string name="app_name_snapshot">Dual Boot Patcher (Snapshot)</string>

    <!-- Toast notification telling user to wait if the back button is pressed while an operation is still running (eg. switching or patching) -->
    <string name="wait_until_finished">Please wait until all operations are finished.</string>

    <!-- Indicate the version of the app on the about screen -->
    <string name="version">Version %s</string>
    <!-- GitHub source code web page URL -->
    <string name="url_source_code" translatable="false">https://github.com/chenxiaolong/DualBootPatcher</string>
    <!-- GitHub source code link text -->
    <string name="link_source_code">Source Code</string>
    <!-- XDA forum thread web page URL -->
    <string name="url_xda_thread" translatable="false">http://forum.xda-developers.com/showthread.php?t=2447534</string>
    <!-- XDA forum thread link text -->
    <string name="link_xda_thread">XDA Thread</string>
    <!-- Licenses URL -->
    <string name="url_licenses" translatable="false">https://github.com/chenxiaolong/DualBootPatcher/tree/master/licenses</string>
    <!-- Licenses link text -->
    <string name="link_licenses">Licenses</string>
    <!-- Thank you message for contributors and links to relavent URLs -->
    <string name="credits">Special thanks to everyone who helped make this patcher possible!</string>

    <!-- Title for card shown in the switcher when the connection to mbtool failed -->
    <string name="mbtool_connect_fail_title">Could not connect to mbtool</string>
    <!-- Message for the card shown in the switcher when the connection to mbtool failed -->
    <string name="mbtool_connect_fail_message">The application was unable to connect to
        mbtool. This could happen if root access was denied or if the ramdisk is not up to
        date.\n\nTap to try again.</string>

    <!-- Navigation drawer item for selecting the ROM -->
    <string name="title_roms">ROMs</string>
    <!-- Navigation drawer item for patching a zip file -->
    <string name="title_patch_zip">Patch Zip File</string>
    <!-- Navigation drawer item for checking free space -->
    <string name="title_free_space">Free Space</string>
    <!-- Navigation drawer item for changing the ROM settings -->
    <string name="title_rom_settings">ROM Settings</string>
    <!-- Navigation drawer item for changing the app sharing settings -->
    <string name="title_app_sharing">App Sharing</string>
    <!-- Nagivation drawer item for rebooting the device -->
    <string name="title_reboot">Reboot</string>
    <!-- Navigation drawer item for showing the about screen -->
    <string name="title_about">About</string>
    <!-- Nagivation drawer item for exiting the app -->
    <string name="title_exit">Exit</string>

    <!-- Accessibility text for opening the app drawer -->
    <string name="drawer_open">Open navigation drawer</string>
    <!-- Accessibility text for closing the app drawer -->
    <string name="drawer_close">Close navigation drawer</string>

    <!-- Default name for the primary ROM -->
    <string name="primary">Primary</string>
    <!-- Default name for the secondary ROM -->
    <string name="secondary">Secondary</string>
    <!-- Default name for the multi-slot ROMs -->
    <string name="multislot">Multi Slot %s</string>
    <!-- Text shown when a ROM's version could not be detected -->
    <string name="couldnt_determine_version">Couldn\'t determine ROM version</string>

    <!-- Title of progress dialog shown when switching ROMs -->
    <string name="switching_rom">Switching ROM</string>
    <!-- Title of progress dialog shown when setting kernel -->
    <string name="setting_kernel">Setting kernel</string>
    <!-- Message shown when switching ROMs succeeds -->
    <string name="choose_rom_success">Successfully switched ROM</string>
    <!-- Message shown when switching ROMs failed -->
    <string name="choose_rom_failure">Failed to switch ROM</string>
    <!-- Message shown when setting the kernel succeeds -->
    <string name="set_kernel_success">Successfully set the kernel</string>
    <!-- Message shown when setting the kernel fails -->
    <string name="set_kernel_failure">Failed to set kernel</string>

    <!-- Version text shown on ROM card if the version could not be detected -->
    <string name="rom_card_unknown_version">Unknown version</string>
    <!-- Build text shown on ROM card if the build could not be detected -->
    <string name="rom_card_unknown_build">Unknown build</string>

    <!-- ROM menu item for setting kernel -->
    <string name="rom_menu_set_kernel">Set kernel</string>
    <!-- ROM menu item for changing the name -->
    <string name="rom_menu_edit_name">Edit name</string>
    <!-- ROM menu item for changing the image -->
    <string name="rom_menu_change_image">Change image</string>
    <!-- ROM menu item for resetting back to the default image -->
    <string name="rom_menu_reset_image">Reset image</string>

    <!-- Accessibility text for the ROM logo images -->
    <string name="switcher_rom_image">ROM logo</string>

    <!-- Title for the confirmation dialog box shown when trying to set the kernel -->
    <string name="switcher_ask_set_kernel_title">Set kernel?</string>
    <!-- Message for the confirmation dialog box shown when trying to set the kernel -->
    <string name="switcher_ask_set_kernel_desc">The currently running kernel will be saved
        as the kernel for %s.</string>

    <!-- Title for the dialog box to rename a ROM -->
    <string name="rename_rom_title">Rename %s</string>
    <!-- Message asking for a new ROM name (blank for the default) for the dialog box to rename a ROM -->
    <string name="rename_rom_desc">Enter a new name for %s. Leave blank to use the
        default name.</string>

    <!-- Title for the patcher card asking for the device and partition configuration -->
    <string name="devpartconfig_title">Device and partition configuration:</string>

    <!-- Title for the dialog box indicating that no file manager is installed -->
    <string name="filemanager_missing_title">No file manager</string>
    <!-- Message telling the user that no file manager is installed and that one needs to be installed to be able to patch a file -->
    <string name="filemanager_missing_desc">There is no suitable file manager installed. Please
        install a file manager to be able to choose files to patch.</string>

    <!-- Title for the file chooser card telling the user to tap to choose a file -->
    <string name="filechooser_initial_title">Tap to choose a file to patch</string>
    <!-- Message telling the user that zip files and boot images are supported by the patcher. If an unsupported file is chosen, the custom patcher options will be shown -->
    <string name="filechooser_initial_desc">If an unsupported file is chosen, additional options
        will appear that allow the patching process to be customized.</string>

    <!-- Title for the file chooser card that is shown after a supported file has been selected. Tells the user to tap to start patching -->
    <string name="filechooser_ready_title">Tap to begin patching</string>
    <!-- Message indicating which file has been chosen and that tapping allows another file to be chosen -->
    <string name="filechooser_ready_desc">Selected file: %s\n\nTap and hold to choose a different
        file.</string>

    <!-- Title for the file chooser card that is shown after an unsupported file has been selected. Tells the user to tap to start patching -->
    <string name="filechooser_unsupported_title">Tap to begin patching</string>
    <!-- Message for the file chooser card showing the selected file and indicating that the file is not supported. The file can still be patched by changing the custom patcher options -->
    <string name="filechooser_unsupported_desc">Selected file: %s\n\nThe file you have selected
        is not supported. You can attempt to patch the file anyway using the options
        below.\n\nTap and hold to choose a different file.</string>

    <!-- Title for the file chooser card indicating that the file was successfully patched -->
    <string name="filechooser_success_title">Successfully patched file</string>
    <!-- Message for the file chooser card showing the newly patched file and telling the user to tap to choose another file to patch -->
    <string name="filechooser_success_desc">New file: %s\n\nTap to choose another file to
        patch.</string>

    <!-- Title for the file chooser card indicating that the patcher failed to patch the file -->
    <string name="filechooser_failure_title">Failed to patch file</string>
    <!-- Title for the file chooser card showing the error message and telling the user to tap to choose another file to patch -->
    <string name="filechooser_failure_desc">%s\n\nTap to choose another file to patch.</string>

    <!-- Title for the preset card -->
    <string name="preset_title">Preset</string>
    <!-- Name for the custom preset item -->
    <string name="preset_custom">Custom</string>
    <!-- Description for the custom preset item indicating that further options can be modified -->
    <string name="preset_custom_desc">Customize the patcher using the options below</string>

    <!-- Title for the custom options card -->
    <string name="customopts_title">Custom options</string>
    <!-- Name for the patch file radio button -->
    <string name="customopts_patchfile">Patch file</string>
    <!-- Name for the button to select a .patch or .diff file -->
    <string name="customopts_choosepatch">Choose patch file</string>
    <!-- Name for the option to disable checks for the device's model -->
    <string name="customopts_devicecheck">Remove device check</string>
    <!-- Name for the option telling the patcher that the file has a boot image -->
    <string name="customopts_hasbootimage">Has boot image</string>
    <!-- Name for the boot image text box -->
    <string name="customopts_bootimage">Boot image</string>
    <!-- Default text for the boot image text box indicating that leaving the box blank will enable autodetection -->
    <string name="customopts_bootimage_hint">Leave blank to autodetect</string>

    <!-- Accessibility text for the checkbox in the progress box -->
    <string name="task_checkbox_image">Task completion checkbox</string>

    <!-- Title for the details card -->
    <string name="details_title">Details:</string>
    <!-- Text for the details card indicating that the patching is done -->
    <string name="details_done">Done</string>

    <!-- Title for the progress card -->
    <string name="overall_progress">Overall progress:</string>
    <!-- Text for the progress card showing how many files have been added to the new zip -->
    <string name="overall_progress_files">%1$s/%2$s files</string>

    <!-- Title for total size of partition -->
    <string name="free_space_total_size">Total size:</string>
    <!-- Title for occupied space on partition -->
    <string name="free_space_occupied_size">Occupied:</string>
    <!-- Title for free space on partition -->
    <string name="free_space_free_size">Free:</string>
    <!-- Format string for gigabytes -->
    <string name="format_gigabytes" translatable="false">%s GB</string>
    <!-- Format string for megabytes -->
    <string name="format_megabytes" translatable="false">%s MB</string>
    <!-- Format string for kilobytes -->
    <string name="format_kilobytes" translatable="false">%s kB</string>
    <!-- Format string for bytes -->
    <string name="format_bytes" translatable="false">%s B</string>

    <!-- Title for the application sharing section of the ROM Settings -->
    <string name="a_s_settings_app_sharing_title">Application sharing (EXPERIMENTAL)</string>
    <!-- Name of the preference for globally sharing free applications from the primary ROM -->
    <string name="a_s_settings_share_apps_title">Share applications</string>
    <!-- Description of the preference for globally shareing free applications from the primary ROM -->
    <string name="a_s_settings_share_apps_desc">Share applications from the primary ROM with the
        current ROM (requires reboot).</string>
    <!-- Name of the preference for globally sharing paid applications from the primary ROM -->
    <string name="a_s_settings_share_paid_apps_title">Share paid applications</string>
    <!-- Description of the preference for globally shareing paid applications from the primary ROM -->
    <string name="a_s_settings_share_paid_apps_desc">
        <![CDATA[
            Share paid applications from the primary ROM with the current ROM (requires reboot).
            <strong>May not work due to DRM.</strong>
        ]]>
    </string>

    <!-- Title for the preference to share individual applications -->
    <string name="a_s_settings_indiv_app_sharing_title">Share individual applications</string>
    <!-- Description for the preference to share individual applications -->
    <string name="a_s_settings_indiv_app_sharing_desc">Share individual applications across ROMs
        (requires reboot).</string>

    <!-- Title for preference category for boot options -->
    <string name="rom_settings_booting_title">Booting options</string>

    <!-- Preference for updating the ramdisk -->
    <string name="update_ramdisk_title">Update ramdisk</string>
    <!-- Description for updating the ramdisk. Avoids having to repatch the ROM -->
    <string name="update_ramdisk_desc">The ramdisk needs to be updated to use the application
        sharing features.</string>
    <!-- Title when ramdisk is successfully updated -->
    <string name="update_ramdisk_success_title">Successfully updated ramdisk</string>
    <!-- Message asking user to reboot when the ramdisk updating succeeds -->
    <string name="update_ramdisk_reboot_desc">The ramdisk was successfully updated. Would you like
        to reboot now?</string>
    <!-- Title when ramdisk failed to update -->
    <string name="update_ramdisk_failure_title">Failed to update ramdisk</string>
    <!-- Message telling the user that app sharing will not work if the ramdisk updating failed -->
    <string name="update_ramdisk_failure_desc">The ramdisk failed to update. Application
        sharing most likely will not work properly.</string>

    <!-- Title for the section in ROM Settings to change settings related to the patcher -->
    <string name="rom_settings_patcher_title">Patcher</string>
    <!-- Title for the preference to show the exit button in the navigation drawer -->
    <string name="rom_settings_show_exit_title">Show Exit button</string>
    <!-- Title for the preference to show the reboot button in the navigation drawer -->
    <string name="rom_settings_show_reboot_title">Show Reboot button</string>

    <!-- Title for the dialog box explaining individual application sharing -->
    <string name="indiv_app_sharing_dialog_title">Individual app sharing</string>
    <!-- Description for the dialog box explaining individual application sharing -->
    <string name="indiv_app_sharing_dialog_desc">Individual app sharing allows the device to keep
        one copy of an app for every selected ROM in order to save space. It also ensures that the app is
        updated in each ROM.\n\nWhen an app is installed, upgraded, or removed, the patcher will
        sometimes request root access to start up the \'syncdaemon\' service. Please allow the
        request for the syncing to succeed.</string>

    <!-- Description for the individual app sharing preference indicating that is cannot be used when global app sharing is enabled. -->
    <string name="indiv_app_sharing_incompat_global">Cannot be used while global application
        sharing is enabled.</string>

    <!-- Description in the app sharing list showing which ROMs the application is installed in -->
    <string name="installed_in_roms">Currently installed in:</string>
    <!-- Description for the checkboxes indicating which ROMs the application should be shared across -->
    <string name="share_apks_across_title">Use the same apk across:</string>

    <!-- Word/phrase for None (used in eg. a list) -->
    <string name="none">None</string>
    <!-- Word/phrase for OK (used in dialog boxes) -->
    <string name="ok">OK</string>
    <!-- Word/phrase for Cancel (used in dialog boxes) -->
    <string name="cancel">Cancel</string>
    <!-- Word/phrase for Continue (used in dialog boxes) -->
    <string name="proceed">Continue</string>
    <!-- Reboot now (used in dialog boxes) -->
    <string name="reboot_now">Reboot now</string>
    <!-- Reboot later (used in dialog boxes) -->
    <string name="reboot_later">Reboot later</string>

    <!-- Text for checkbox in dialogs indicating that the dialog should not be shown again -->
    <string name="do_not_show_again">Do not show again</string>
    <!-- Text shown in a progress dialog asking the user to wait -->
    <string name="please_wait">Please wait</string>

    <!-- Presets that should always be visible -->
    <string-array name="preset_dirs" translatable="false">
        <item>Google_Apps</item>
        <item>Other</item>
    </string-array>

